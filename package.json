{
  "name": "axios-etag-cache",
  "version": "1.2.1",
  "description": "Axios etag interceptor to enable If-None-Match request with ETAG support",
  "keywords": [
    "axios",
    "cache",
    "etag"
  ],
  "homepage": "https://github.com/raulanatol/axios-etag-cache#readme",
  "bugs": {
    "url": "https://github.com/raulanatol/axios-etag-cache/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raulanatol/axios-etag-cache.git"
  },
  "license": "ISC",
  "author": {
    "name": "raulanatol",
    "email": "raul@natol.es"
  },
  "exports": {
    ".": {
      "import": "./dist/zzm-base.es.js",
      "require": "./dist/zzm-base.umd.js"
    }
  },
  "main": "./dist/zzm-base.umd.js",
  "module": "./dist/zzm-base.es.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "analyze-build": "tsc && BUNDLE_ANALYZE=1 vite build",
    "build": "tsc && vite build",
    "clean": "rm -rf dist build",
    "lint": "eslint --max-warnings 0 . --ext .ts",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "devDependencies": {
    "@cowcoders/eslint-config": "1.5.5",
    "@types/axios": "0.14.0",
    "@types/jest": "28.1.2",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "axios": "0.27.2",
    "eslint": "^8.18.0",
    "jest": "28.1.1",
    "nock": "13.2.7",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "shx": "0.3.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.4",
    "vite": "2.9.12",
    "vite-plugin-dts": "1.2.0",
    "webserver.local": "^1.0.5"
  },
  "peerDependencies": {
    "axios": ">=0.24.0"
  },
  "pre-push": [
    "lint",
    "test",
    "build"
  ]
}
